{
    "yaml.schemas" : {
    "mig_yaml_schema.json": ["/config/*.yaml.j2"] 
    },

   "diagnostic.checkCurrentLine": true,


   "python.autoComplete..extraPaths": [],

  "languageserver": {
    "go": {
      "command": "~/go/bin/gopls",
      "rootPatterns": ["go.work", "go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
    }
  }



}


